# Copy this file to .env and fill in your credentials and schedule settings.
BLUESKY_USERNAME=example.bsky.social
BLUESKY_PASSWORD=
# Frequency of runs in minutes (must be >0)
RUN_INTERVAL_MINUTES=1440 # 1 day
# Optional: seconds to wait before the first run begins
FIRST_RUN_DELAY_SECONDS=30
# Optional: true for -vv logging, false for -v
VERY_VERBOSE_LOGGING=false

#### YOU MUST SUPPLY ONE OF THE FIRST TWO VARS HERE
# Value passed to skeeter-deleter.py -s flag (days)
STALE_LIMIT_DAYS=
# Value for --max-reposts (-l)
MAX_REPOSTS=

# Optional bot recommended: cursor for --fixed-likes-cursor (-c)
FIXED_LIKES_CURSOR=

# Optional: comma-separated domains for --domains-to-protect (-d)
DOMAINS_TO_PROTECT=
